<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.longecological.mapper.system.SysChannelMapper">

	<!-- 查询渠道详情 -->
	<select id="getSysChannelById" parameterType="java.lang.String" resultType="java.util.Map">
		select * from t_sys_channel where id = #{id}
	</select>
	
	<!-- 查询列表 -->
	<select id="getSysChannelList" resultType="java.util.Map">
		select * from t_sys_channel
	</select>

</mapper>